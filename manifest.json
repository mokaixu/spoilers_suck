{
	"name": "Spoilers Suck",
	"description": "Don't let your dank friends spoil your favourite shows",
	"version": "1.0",
	"manifest_version": 2,
	"icons": {
    "16": "public/img/night-king.jpg"
        
	},


	"permissions": [
		"tabs", "http://*/*", "activeTab", "storage", "debugger", "contextMenus"
	],

	"browser_action": {
